;;; -*- lexical-binding: t -*-
(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(column-number-mode t)
 '(disable-mouse-mode-lighter "")
 '(ediff-window-setup-function 'ediff-setup-windows-plain)
 '(js-indent-level 8)
 '(package-selected-packages nil)
 '(package-vc-selected-packages
   '((auto-hide :url "https://github.com/BlameTroi/auto-hide.el"
		:branch "main")))
 '(tool-bar-mode nil)
 '(xref-search-program 'ripgrep))
(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(compilation-error ((t (:background "gray80" :foreground "Red"))))
 '(flymake-error ((t (:underline (:color "Red" :style wave :position nil)))))
 '(font-lock-comment-face ((t (:foreground "#707070" :slant oblique))))
 '(fringe ((t :background "#FFFFE8")))
 '(header-line ((t :box (:line-width 4 :color "#e1faff" :style nil))))
 '(header-line-highlight ((t :box (:color "#444444"))))
 '(hl-line ((t (:inherit highlight :extend t :background "LightGoldenrod2" :foreground "black"))))
 '(keycast-key ((t)))
 '(line-number ((t :background "#FFFFE8")))
 '(mode-line ((t :box (:line-width 6 :color "#e1faff" :style nil))))
 '(mode-line-active ((t :box (:line-width 6 :color "#e1faff" :style nil))))
 '(mode-line-highlight ((t :box (:color "#444444"))))
 '(mode-line-inactive ((t :box (:line-width 6 :color "#E5E5D0" :style nil))))
 '(tab-bar-tab ((t :box (:line-width 4 :color "grey85" :style nil))))
 '(tab-bar-tab-inactive ((t :box (:line-width 4 :color "grey75" :style nil))))
 '(tab-line-tab ((t)))
 '(tab-line-tab-active ((t)))
 '(tab-line-tab-inactive ((t)))
 '(vertical-border ((t :background "#FFFFE8" :foreground "#FFFFE8")))
 '(window-divider ((t (:background "#FFFFE8" :foreground "#FFFFE8"))))
 '(window-divider-first-pixel ((t (:background "#FFFFE8" :foreground "#FFFFE8"))))
 '(window-divider-last-pixel ((t (:background "#FFFFE8" :foreground "#FFFFE8")))))
